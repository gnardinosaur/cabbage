<h1>Sign Up</h1>

<%= form_for @user do |f| %>
    First Name: <%= f.text_field :first_name %><br />
    
    <%= f.label :last_name %>
    <%= f.text_field :last_name %><br />

    <%= f.label :username %>
    <%= f.text_field :username %><br />

    <%= f.label :password %>
    <%= f.text_field :password %><br />

    <%= f.submit %>
    
<% end %>